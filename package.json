    "@vitest/coverage-v8": "^0.32.0",
    "@waylaidwanderer/fetch-event-source": "^3.0.1",
    "crypto-js": "^4.1.1",
    "firebase": "^9.20.0",
    "firebase-tools": "^12.0.0",
    "firebaseui": "^6.0.2",
    "form-data": "^4.0.0",
    "franc": "^6.1.0",
    "gpt-tokenizer": "^2.1.1",
    "js-tiktoken": "^1.0.7",
    "langchain": "^0.0.114",
    "lodash": "^4.17.21",
    "lru-cache": "^9.1.2",
    "markdown-it": "^13.0.1",
    "openai": "^3.2.1",
    "pinia": "^2.0.11",
    "pino": "^8.14.1",
    "prismjs": "^1.29.0",
    "quasar": "^2.6.0",
